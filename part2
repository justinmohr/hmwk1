OPEN DATA "E:\Justin\Documents\USF\6_Spring 2018\Forcasting\hw1\HW1.xls"
CALENDAR(M) 1999:1
DATA(FORMAT=XLS,ORG=COLUMNS,LEFT=2) 1999:01 2017:12 CAD JPY USD ICAD IJPY IUSD
GRAPH(STYLE=LINE,HEADER="Indexed Graph",KEY=UPLEFT) 3
# ICAD
# IJPY
# IUSD
SET MGRCAD = (CAD/CAD{1}-1)
SET MGRJPY = (JPY/JPY{1}-1)
SET MGRUSD = (USD/USD{1}-1)
GRAPH(STYLE=LINE,HEADER="Monthly Growth Rates",KEY=UPLEFT) 3
# MGRCAD
# MGRJPY
# MGRUSD
SET AGRCAD = (CAD/CAD{12}-1) 
SET AGRJPY = (JPY/JPY{12}-1) 
SET AGRUSD = (USD/USD{12}-1) 
GRAPH(STYLE=LINE,HEADER="Annual Growth Rate",KEY=UPLEFT) 3
# AGRCAD
# AGRJPY
# AGRUSD
SET LNCAD = LOG(CAD)
SET LNJPY = LOG(JPY)
SET LNUSD = LOG(USD)
DIFF LNCAD / DIFLNCAD
DIFF LNJPY / DIFLNJPY
DIFF LNUSD / DIFLNUSD
GRAPH(STYLE=LINE,HEADER="LN First-Difference",KEY=UPLEFT) 3
# DIFLNCAD
# DIFLNJPY
# DIFLNUSD
